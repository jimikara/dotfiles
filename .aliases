# Easier navigation: .., ..., ~ and -
alias ..="cd .."
alias cd..="cd .."
alias ...="cd ../.."
alias ....="cd ../../.."
alias .....="cd ../../../.."
alias ~="cd ~" # `cd` is probably faster to type though
alias -- -="cd -"

# My popular directories

alias sites="cd/Documents/sites/"

# Seed specifc navigation shortcuts
alias lara="cd ~/Documents/sites/seed/Larahub"
